
@{
    ViewData["Title"] = "result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .dez-bnr-inr {
        height: 340px;
        background-size: cover;
        background-position: bottom;
        display: table;
        width: 100%;
        text-align: center;
    }
    .content-inner {
        padding-top: 24px;
        padding-bottom: 40px;
    }
    .header-nav .nav > li .sub-menu {
        background-color: #ffffff;
        display: block;
        left: 0;
        list-style: none;
        opacity: 0;
        padding: 5px 0;
        position: absolute;
        visibility: hidden;
        width: 298px;
        z-index: 10;
    }
</style>

<div class="dez-bnr-inr" style="background-image:url(https://uploads.yogaintown.co.uk/images/bg9.jpg);">
    <div class="container">
        <div class="dez-bnr-inr-entry">
            <h1 style="color: #6a4b21; margin-bottom: 0px;">Result</h1>
        </div>
    </div>
</div>
<!-- inner page banner END -->
<!-- Breadcrumb row -->
<div class="breadcrumb-row">
    <div class="container">
        <ul class="list-inline">
            <li><a href="/home/homepage">Home</a></li>
            <li>Result</li>
        </ul>
    </div>
</div>
<!-- Breadcrumb row END -->
<!-- Content -->
<div class="section-full content-inner bg-white">
    <!-- contact area -->
    <div class="container">
        <!-- 404 Page -->
        <div class="section-content">
            <div class="row">
                <div class="col-lg-12">
                    @if (ViewBag.Type == 0)
                    {
                        <div class="page-notfound error-box text-center">
                            <h2 class="error-text">Oops!</h2>
                            <p class="mb-4">@ViewBag.Result</p>
                            <a href="/home/homepage" class="site-button">Back To Home</a>
                        </div>
                    }
                    else if (ViewBag.Type == 1)
                    { <div class="page-notfound error-box text-center">
                            <h2 class="text-success">Successful!</h2>
                            <p class="mb-4">@ViewBag.Result</p>
                            <a href="/home/homepage" class="site-button">Back To Home</a>
                        </div>
                    }
                </div>
            </div>
        </div>
        <!-- 404 Page END -->
    </div>
    <!-- contact area  END -->
</div>
<!-- Content END-->
